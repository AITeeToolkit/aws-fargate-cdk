{
    "branches": [
      "main"
    ],
    "repositoryUrl": "https://github.com/AITeeToolkit/aws-fargate-cdk.git",
    "tagFormat": "v${version}",
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits",
          "releaseRules": [
            { "type": "feat", "release": "minor" },
            { "type": "fix", "release": "patch" },
            { "type": "perf", "release": "patch" },
            { "type": "revert", "release": "patch" },
            { "type": "docs", "release": "patch" },
            { "type": "style", "release": false },
            { "type": "chore", "release": false },
            { "type": "refactor", "release": "patch" },
            { "type": "test", "release": false },
            { "type": "build", "release": "patch" },
            { "type": "ci", "release": false }
          ]
        }
      ],
      [
        "@semantic-release/release-notes-generator",
        {
          "preset": "conventionalcommits",
          "presetConfig": {
            "types": [
              { "type": "feat", "section": "ðŸš€ Features" },
              { "type": "fix", "section": "ðŸ› Bug Fixes" },
              { "type": "perf", "section": "âš¡ Performance Improvements" },
              { "type": "revert", "section": "âª Reverts" },
              { "type": "docs", "section": "ðŸ“š Documentation" },
              { "type": "refactor", "section": "â™»ï¸ Code Refactoring" },
              { "type": "build", "section": "ðŸ—ï¸ Build System" }
            ]
          }
        }
      ],
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md",
          "changelogTitle": "# Changelog\n\nAll notable changes to this project will be documented in this file. See [Conventional Commits](https://conventionalcommits.org) for commit guidelines."
        }
      ],
      [
        "@semantic-release/github",
        {
          "successComment": "ðŸŽ‰ This ${issue.pull_request ? 'PR is included' : 'issue has been resolved'} in version ${nextRelease.version} :tada:",
          "failTitle": "âŒ The automated release failed",
          "failComment": "The automated release from the `${branch.name}` branch failed. :x:\n\n${errors.map(err => err.message).join('\\n\\n')}",
          "labels": ["released"],
          "addReleases": "bottom"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": ["CHANGELOG.md", "package.json", "package-lock.json"],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
  